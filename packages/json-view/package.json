{
    "name": "nxt-json-view",
    "description": "Angular component for interactive json view.",
    "bugs": "https://github.com/Liquid-JS/nxt-components/issues",
    "version": "14.0.0",
    "license": "MIT",
    "repository": "https://github.com/Liquid-JS/nxt-components.git",
    "scripts": {
        "watch": "ng build json-view --configuration \"development\" --watch",
        "build": "ng build json-view --configuration \"production\"",
        "lint": "ng lint json-view",
        "lint:fix": "yarn lint --fix",
        "lint:html": "js-beautify --type html -r -f ./src/**/*.html --config ./js-beautify.json",
        "lint:sass": "stylelint \"src/**/*.scss\"",
        "lint:sass:fix": "yarn lint:sass --fix",
        "test": "ng test json-view --no-watch",
        "test:watch": "ng test json-view"
    },
    "peerDependencies": {
        "@angular/common": "^15.2.1",
        "@angular/core": "^15.2.1"
    },
    "dependencies": {
        "tslib": "^2.5.0"
    }
}
