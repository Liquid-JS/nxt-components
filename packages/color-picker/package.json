{
    "name": "nxt-color-picker",
    "description": "Color picker widget for Angular",
    "bugs": "https://github.com/Liquid-JS/nxt-components/issues",
    "version": "16.0.0-alpha.5",
    "license": "MIT",
    "repository": "https://github.com/Liquid-JS/nxt-components.git",
    "homepage": "https://github.com/Liquid-JS/nxt-components/tree/master/packages/color-picker",
    "scripts": {
        "watch": "ng build color-picker --configuration \"development\" --watch",
        "build": "ng build color-picker --configuration \"production\"",
        "lint": "ng lint color-picker",
        "lint:fix": "yarn lint --fix",
        "lint:html": "js-beautify --type html -r -f ./src/**/*.html --config ./js-beautify.json",
        "lint:sass": "stylelint \"src/**/*.scss\"",
        "lint:sass:fix": "yarn lint:sass --fix",
        "test": "ng test color-picker --no-watch",
        "test:watch": "ng test color-picker",
        "docs": "npx compodoc -c .compodocrc.json",
        "docs:json": "yarn docs --exportFormat json --output ../demo/src/app/color-picker"
    },
    "peerDependencies": {
        "@angular/cdk": "^16.0.0-next.2",
        "@angular/common": "^16.0.0-next.4",
        "@angular/core": "^16.0.0-next.4",
        "@angular/localize": "^16.0.0-next.4"
    },
    "dependencies": {
        "tslib": "^2.5.0"
    }
}
