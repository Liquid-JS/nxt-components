{
    "name": "nxt-flags",
    "description": "Angular component to show country flags.",
    "bugs": "https://github.com/Liquid-JS/nxt-components/issues",
    "version": "14.0.0",
    "license": "MIT",
    "repository": "https://github.com/Liquid-JS/nxt-components.git",
    "scripts": {
        "watch": "ng build flags --configuration \"development\" --watch",
        "build": "ng build flags --configuration \"production\"",
        "lint": "ng lint flags",
        "lint:fix": "yarn lint --fix",
        "lint:html": "js-beautify --type html -r -f ./src/**/*.html --config ./js-beautify.json",
        "lint:sass": "stylelint \"src/**/*.scss\"",
        "lint:sass:fix": "yarn lint:sass --fix",
        "test": "ng test flags --no-watch",
        "test:watch": "ng test flags"
    },
    "peerDependencies": {
        "@angular/common": "^14.2.0",
        "@angular/core": "^14.2.0"
    },
    "dependencies": {
        "tslib": "^2.3.0"
    }
}
