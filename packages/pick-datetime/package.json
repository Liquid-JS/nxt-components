{
    "name": "nxt-pick-datetime",
    "description": "Date time picker for Angular",
    "bugs": "https://github.com/Liquid-JS/nxt-components/issues",
    "version": "15.0.0",
    "license": "MIT",
    "repository": "https://github.com/Liquid-JS/nxt-components.git",
    "homepage": "https://github.com/Liquid-JS/nxt-components/tree/master/packages/pick-datetime",
    "scripts": {
        "watch": "ng build pick-datetime --configuration \"development\" --watch",
        "build": "ng build pick-datetime --configuration \"production\"",
        "lint": "ng lint pick-datetime",
        "lint:fix": "yarn lint --fix",
        "lint:html": "js-beautify --type html -r -f ./src/**/*.html --config ./js-beautify.json",
        "lint:sass": "stylelint \"src/**/*.scss\"",
        "lint:sass:fix": "yarn lint:sass --fix",
        "test": "ng test pick-datetime --no-watch",
        "test:watch": "ng test pick-datetime",
        "docs": "npx compodoc -c .compodocrc.json",
        "docs:json": "yarn docs --exportFormat json --output ../demo/src/app/pick-datetime"
    },
    "peerDependencies": {
        "@angular/animations": "^15.2.2",
        "@angular/cdk": "^15.2.2",
        "@angular/common": "^15.2.2",
        "@angular/core": "^15.2.2",
        "@angular/forms": "^15.2.2",
        "@angular/localize": "^15.2.2",
        "moment": "^2.29.4",
        "rxjs": "^7.8.0"
    },
    "dependencies": {
        "tslib": "^2.5.0"
    },
    "exports": {
        "./assets/picker.scss": {
            "default": "./assets/picker.scss"
        }
    }
}
