{
    "name": "nxt-components",
    "version": "0.0.0",
    "private": true,
    "repository": "https://github.com/Liquid-JS/nxt-components.git",
    "author": "HitkoDev <development@hitko.si>",
    "license": "MIT",
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    },
    "scripts": {
        "postinstall": "husky install",
        "build": "yarn workspaces foreach -viptA --exclude demo run build",
        "build:demo": "yarn workspace demo build",
        "serve": "yarn workspace demo serve",
        "watch": "yarn workspaces foreach -vipA --exclude demo run watch",
        "test": "yarn workspaces foreach -vipA run test",
        "lint": "yarn workspaces foreach -vipA run lint",
        "lint:fix": "yarn workspaces foreach -vipA run lint:fix",
        "lint:html": "yarn workspaces foreach -vipA run lint:html",
        "lint:sass": "yarn workspaces foreach -vipA run lint:sass",
        "lint:sass:fix": "yarn workspaces foreach -vipA run lint:sass:fix",
        "docs": "yarn workspaces foreach -vipA run docs && yarn workspaces foreach -vipA run docs:json",
        "pages": "rimraf docs && yarn docs && yarn build:demo && yarn scully:demo && yarn scripts:add404",
        "pages:publish": "yarn pages && ngh --dir docs",
        "scripts:add404": "node --loader ts-node/esm scripts/add-404.ts",
        "scripts:min-version": "node --loader ts-node/esm scripts/min-version.ts",
        "scully:demo": "scully --project demo --scanRoutes --noPrompt --prod",
        "scully:serve": "scully serve --project demo"
    },
    "dependencies": {
        "@angular/animations": "^18.0.1",
        "@angular/cdk": "^18.0.1",
        "@angular/common": "^18.0.1",
        "@angular/compiler": "^18.0.1",
        "@angular/core": "^18.0.1",
        "@angular/forms": "^18.0.1",
        "@angular/localize": "^18.0.1",
        "@angular/platform-browser": "^18.0.1",
        "@angular/platform-server": "^18.0.1",
        "dropzone": "^6.0.0-beta.2",
        "moment": "^2.30.1",
        "rxjs": "^7.8.1",
        "sortablejs": "^1.15.2",
        "tslib": "^2.6.2",
        "zone.js": "^0.14.6"
    },
