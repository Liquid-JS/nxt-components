{
    "name": "nxt-pick-datetime",
    "version": "0.0.0",
    "scripts": {
        "start": "ng serve --disable-host-check",
        "build": "ng build --configuration production",
        "build:demo": "ng build --configuration production --localize --base-href https://liquid-js.github.io/nxt-pick-datetime/ demo",
        "analyze": "yarn build:demo --stats-json && webpack-bundle-analyzer dist/demo/stats.json",
        "test": "ng test --code-coverage --source-map false --watch true",
        "lint": "ng lint --fix",
        "lint:sass": "sass-convert --indent 4 --to scss --from scss --recursive --in-place projects/*/src",
        "pages": "cross-env NODE_ENV=production yarn build:demo && cross-env NODE_ENV=production yarn docs && cross-env NODE_ENV=production ngh --dir dist/demo",
        "docs": "typedoc --tsconfig ./projects/nxt-pick-datetime/tsconfig.lib.json --entryPoints ./projects/nxt-pick-datetime/src/public-api.ts --out ./dist/demo/docs"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "<15.0.0",
        "@angular/cdk": "<15.0.0",
        "@angular/common": "<15.0.0",
        "@angular/compiler": "<15.0.0",
        "@angular/core": "<15.0.0",
        "@angular/forms": "<15.0.0",
        "@angular/platform-browser": "<15.0.0",
        "@angular/platform-browser-dynamic": "<15.0.0",
        "@angular/router": "<15.0.0",
        "@babel/core": "^7.20.12",
        "@babel/generator": "^7.20.7",
        "@babel/preset-env": "^7.20.2",
        "moment": "^2.29.4",
        "rxjs": "^7",
        "tslib": "^2.4.1",
        "zone.js": "^0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "<15.0.0",
        "@angular-eslint/builder": "<15.0.0",
        "@angular-eslint/eslint-plugin": "<15.0.0",
        "@angular-eslint/eslint-plugin-template": "<15.0.0",
        "@angular-eslint/schematics": "<15.0.0",
        "@angular-eslint/template-parser": "<15.0.0",
        "@angular/cli": "<15.0.0",
        "@angular/compiler-cli": "<15.0.0",
        "@angular/language-service": "<15.0.0",
        "@angular/localize": "<15.0.0",
        "@types/jasmine": "^4.3.1",
        "@types/jasminewd2": "^2.0.10",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "angular-cli-ghpages": "^1.0.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.31.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.6.4",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jasmine-core": "^4.5.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "^3.1.1",
        "karma-coverage": "^2.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "ng-packagr": "<15.0.0",
        "protractor": "^7.0.0",
        "puppeteer": "^19.5.0",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.24",
        "typescript": ">=4.8 <4.9",
        "webpack-bundle-analyzer": "^4.7.0"
    },
    "resolutions": {
        "@babel/core": "^7.20.12",
        "@babel/generator": "^7.20.7",
        "@babel/preset-env": "^7.20.2",
        "typescript": ">=4.8 <4.9"
    }
}
