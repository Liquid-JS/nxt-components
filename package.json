{
  "name": "ngx-dropzone-wrapper",
  "description": "Angular wrapper library for Dropzone",
  "bugs": "https://github.com/zefoy/ngx-dropzone-wrapper/issues",
  "version": "10.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --configuration production",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "eslint": {
    "extends": "./.eslintrc.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zefoy/ngx-dropzone-wrapper.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-dropzone-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --prod -- --base-href=ngx-dropzone-wrapper --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "12.0.0-beta.35",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "dropzone": "^5.7.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.15",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^12.2.15",
    "@angular/compiler-cli": "^12.2.16",
    "@types/dropzone": "^5.7.0",
    "@types/node": "^12.12.59",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "mkdirp": "^1.0.0",
    "ng-packagr": "^12.2.6",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "terser": "^5.3.0",
    "ts-node": "^9.0.0",
    "tsickle": "^0.39.0",
    "typescript": "~4.3.5",
    "watch": "^1.0.0",
    "zone.js": "~0.11.4"
  }
}