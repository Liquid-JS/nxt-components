{
  "name": "nxt-dropzone-wrapper",
  "description": "Angular wrapper library for Dropzone",
  "bugs": "https://github.com/Liquid-JS/nxt-dropzone-wrapper/issues",
  "version": "13.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --configuration production",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "eslint": {
    "extends": "./.eslintrc.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Liquid-JS/nxt-dropzone-wrapper.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:Liquid-JS/nxt-dropzone-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --configuration production -- --base-href=/nxt-dropzone-wrapper/ --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "HitkoDev",
        "email": "development@hitko.si"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^13.2.0",
    "@angular/common": "^13.2.0",
    "@angular/compiler": "^13.2.0",
    "@angular/core": "^13.2.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/platform-browser": "^13.2.0",
    "@angular/platform-browser-dynamic": "^13.2.0",
    "dropzone": "^5.9.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.0",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.2.0",
    "@angular/compiler-cli": "^13.2.0",
    "@types/dropzone": "^5.7.4",
    "@types/node": "<17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "core-js": "^3.20.3",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.4.1",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^13.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "terser": "^5.10.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": ">=4.5 <4.6",
    "watch": "^1.0.2",
    "zone.js": "^0.11.4"
  }
}
