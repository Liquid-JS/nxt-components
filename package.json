{
    "name": "nxt-pick-datetime",
    "version": "0.0.0",
    "scripts": {
        "start": "ng serve --disable-host-check",
        "build": "ng build --configuration production",
        "build:demo": "ng build --configuration production --localize --base-href https://liquid-js.github.io/nxt-pick-datetime/ demo",
        "analyze": "yarn build:demo --stats-json && webpack-bundle-analyzer dist/demo/stats.json",
        "test": "ng test --code-coverage --source-map false --watch true",
        "lint": "ng lint --fix",
        "lint:sass": "sass-convert --indent 4 --to scss --from scss --recursive --in-place projects/*/src",
        "pages": "cross-env NODE_ENV=production yarn build:demo && cross-env NODE_ENV=production yarn docs && cross-env NODE_ENV=production ngh --dir dist/demo",
        "docs": "typedoc --tsconfig ./projects/nxt-pick-datetime/tsconfig.lib.json --entryPoints ./projects/nxt-pick-datetime/src/public-api.ts --out ./dist/demo/docs"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "13.0.2",
        "@angular/cdk": "^13.0.2",
        "@angular/common": "13.0.2",
        "@angular/compiler": "13.0.2",
        "@angular/core": "13.0.2",
        "@angular/forms": "13.0.2",
        "@angular/platform-browser": "13.0.2",
        "@angular/platform-browser-dynamic": "13.0.2",
        "@angular/router": "13.0.2",
        "@babel/core": "^7.14.3",
        "@babel/generator": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "moment": "^2.29.1",
        "rxjs": "<7",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.0.3",
        "@angular-eslint/builder": "13.0.1",
        "@angular-eslint/eslint-plugin": "13.0.1",
        "@angular-eslint/eslint-plugin-template": "13.0.1",
        "@angular-eslint/schematics": "13.0.1",
        "@angular-eslint/template-parser": "13.0.1",
        "@angular/cli": "13.0.3",
        "@angular/compiler-cli": "13.0.2",
        "@angular/language-service": "13.0.2",
        "@angular/localize": "13.0.2",
        "@types/jasmine": "~3.10.2",
        "@types/jasminewd2": "^2.0.10",
        "@types/node": "^16.11.9",
        "@typescript-eslint/eslint-plugin": "5.4.0",
        "@typescript-eslint/parser": "5.4.0",
        "angular-cli-ghpages": "^1.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.3.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "latest",
        "eslint-plugin-prefer-arrow": "latest",
        "jasmine-core": "~3.10.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.3.9",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.7.0",
        "ng-packagr": "13.0.6",
        "protractor": "^7.0.0",
        "puppeteer": "^11.0.0",
        "ts-node": "^10.4.0",
        "typedoc": "^0.22.9",
        "typescript": ">=4.4 <4.5",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "resolutions": {
        "@babel/core": "^7.14.3",
        "@babel/generator": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "typescript": ">=4.2 <4.3"
    }
}
